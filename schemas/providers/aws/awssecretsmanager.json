{
  "name": "AwsSecretsManager",
  "kind": "AwsSecretsManager",
  "cloudProvider": "aws",
  "apiVersion": "aws.openmcf.org/v1",
  "description": "aws-secrets-manager",
  "protoPackage": "org.openmcf.provider.aws.awssecretsmanager.v1",
  "protoFiles": {
    "api": "org/openmcf/provider/aws/awssecretsmanager/v1/api.proto",
    "spec": "org/openmcf/provider/aws/awssecretsmanager/v1/spec.proto"
  },
  "spec": {
    "name": "AwsSecretsManagerSpec",
    "fields": [
      {
        "name": "Region",
        "jsonName": "region",
        "protoField": "region",
        "type": {
          "kind": "string"
        },
        "description": "The AWS region where the resource will be created.\n Example: \"us-west-2\", \"eu-west-1\"",
        "required": false,
        "validation": {
          "minLength": 1
        }
      },
      {
        "name": "SecretNames",
        "jsonName": "secretNames",
        "protoField": "secret_names",
        "type": {
          "kind": "array",
          "elementType": {
            "kind": "string"
          }
        },
        "description": "List of secret names to create in AWS Secrets Manager.\n Each name corresponds to a unique secret that will be securely stored and managed.\n Secret names must be unique within your AWS account and region.",
        "required": false,
        "validation": {
          "minItems": 1,
          "unique": true
        }
      }
    ]
  }
}
