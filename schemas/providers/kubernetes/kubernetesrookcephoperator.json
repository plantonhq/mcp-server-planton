    {
      "name": "KubernetesRookCephOperatorCsiSpec",
      "description": "**KubernetesRookCephOperatorCsiSpec** configures the Ceph CSI drivers managed by the Rook operator.\n CSI drivers enable Kubernetes to use Ceph for persistent volume claims.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetesrookcephoperator.v1.KubernetesRookCephOperatorCsiSpec",
      "fields": [
        {
          "name": "EnableRbdDriver",
          "jsonName": "enableRbdDriver",
          "protoField": "enable_rbd_driver",
          "type": {
            "kind": "bool"
          },
          "description": "Enable the Ceph CSI RBD (block storage) driver.\n Default: true",
          "required": false,
          "default": "true",
          "oneofGroup": "_enable_rbd_driver"
        },
        {
          "name": "EnableCephfsDriver",
          "jsonName": "enableCephfsDriver",
          "protoField": "enable_cephfs_driver",
          "type": {
            "kind": "bool"
          },
          "description": "Enable the Ceph CSI CephFS (file storage) driver.\n Default: true",
          "required": false,
          "default": "true",
          "oneofGroup": "_enable_cephfs_driver"
        },
        {
          "name": "DisableCsiDriver",
          "jsonName": "disableCsiDriver",
          "protoField": "disable_csi_driver",
          "type": {
            "kind": "bool"
          },
          "description": "Disable the CSI driver entirely.\n Set to true if you want to use an external CSI driver.\n Default: false",
          "required": false,
          "default": "false",
          "oneofGroup": "_disable_csi_driver"
        },
        {
          "name": "EnableCsiHostNetwork",
          "jsonName": "enableCsiHostNetwork",
          "protoField": "enable_csi_host_network",
          "type": {
            "kind": "bool"
          },
          "description": "Enable host networking for CSI CephFS and RBD nodeplugins.\n May be necessary in some network configurations where the SDN does not\n provide access to an external cluster or there is significant drop in read/write performance.\n Default: true",
          "required": false,
          "default": "true",
          "oneofGroup": "_enable_csi_host_network"
        },
        {
          "name": "ProvisionerReplicas",
          "jsonName": "provisionerReplicas",
          "protoField": "provisioner_replicas",
          "type": {
            "kind": "int32"
          },
          "description": "Number of replicas for CSI provisioner deployment.\n Default: 2",
          "required": false,
          "default": "2",
          "oneofGroup": "_provisioner_replicas"
        },
        {
          "name": "EnableCsiAddons",
          "jsonName": "enableCsiAddons",
          "protoField": "enable_csi_addons",
          "type": {
            "kind": "bool"
          },
          "description": "Enable CSI Addons for additional CSI functionality.\n Default: false",
          "required": false,
          "default": "false",
          "oneofGroup": "_enable_csi_addons"
        },
        {
          "name": "EnableNfsDriver",
          "jsonName": "enableNfsDriver",
          "protoField": "enable_nfs_driver",
          "type": {
            "kind": "bool"
          },
          "description": "Enable NFS CSI driver for NFS storage support.\n Default: false",
          "required": false,
          "default": "false",
          "oneofGroup": "_enable_nfs_driver"
        }
      ]
    },
