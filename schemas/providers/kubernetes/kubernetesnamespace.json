    {
      "name": "KubernetesNamespaceNetworkConfig",
      "description": "*\n **KubernetesNamespaceNetworkConfig** defines network isolation and security policies.\n This implements the \"Default Deny\" security pattern with explicit allow rules.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetesnamespace.v1.KubernetesNamespaceNetworkConfig",
      "fields": [
        {
          "name": "IsolateIngress",
          "jsonName": "isolateIngress",
          "protoField": "isolate_ingress",
          "type": {
            "kind": "bool"
          },
          "description": "*\n Enable ingress network isolation.\n When true, creates a NetworkPolicy that denies all ingress traffic by default.\n Traffic must be explicitly allowed.",
          "required": false
        },
        {
          "name": "RestrictEgress",
          "jsonName": "restrictEgress",
          "protoField": "restrict_egress",
          "type": {
            "kind": "bool"
          },
          "description": "*\n Enable egress network restriction.\n When true, blocks all egress except to kube-system (DNS) and Kubernetes API.\n This prevents pods from accessing external networks without explicit rules.",
          "required": false
        },
        {
          "name": "AllowedIngressNamespaces",
          "jsonName": "allowedIngressNamespaces",
          "protoField": "allowed_ingress_namespaces",
          "type": {
            "kind": "array",
            "elementType": {
              "kind": "string"
            }
          },
          "description": "*\n Allow ingress from specific namespaces.\n List of namespace names that are allowed to send traffic to this namespace.",
          "required": false
        },
        {
          "name": "AllowedEgressCidrs",
          "jsonName": "allowedEgressCidrs",
          "protoField": "allowed_egress_cidrs",
          "type": {
            "kind": "array",
            "elementType": {
              "kind": "string"
            }
          },
          "description": "*\n Allow egress to specific external CIDR blocks.\n List of CIDR blocks (e.g., \"10.0.0.0/8\", \"192.168.1.0/24\") that pods can access.",
          "required": false
        },
        {
          "name": "AllowedEgressDomains",
          "jsonName": "allowedEgressDomains",
          "protoField": "allowed_egress_domains",
          "type": {
            "kind": "array",
            "elementType": {
              "kind": "string"
            }
          },
          "description": "*\n Allow egress to specific DNS domains.\n List of domains (e.g., \"api.stripe.com\", \"*.github.com\") that pods can access.\n Requires a CNI that supports DNS-based policies (like Calico or Cilium).",
          "required": false
        }
      ]
    },
    {
      "name": "KubernetesNamespaceObjectCountQuotas",
      "description": "*\n **KubernetesNamespaceObjectCountQuotas** defines limits on Kubernetes object counts.\n This prevents control plane exhaustion from excessive object creation.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetesnamespace.v1.KubernetesNamespaceObjectCountQuotas",
      "fields": [
