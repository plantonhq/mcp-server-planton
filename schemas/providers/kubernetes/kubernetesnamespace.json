{
  "name": "KubernetesNamespace",
  "kind": "KubernetesNamespace",
  "cloudProvider": "kubernetes",
  "apiVersion": "kubernetes.openmcf.org/v1",
  "description": "*\n **KubernetesNamespace** defines a Kubernetes namespace resource following the Kubernetes Resource Model (KRM).\n This resource represents a complete namespace deployment with resource quotas, network policies,\n and access controls configured according to the \"Namespace-as-a-Service\" pattern.",
  "protoPackage": "org.openmcf.provider.kubernetes.kubernetesnamespace.v1",
  "protoFiles": {
    "api": "org/openmcf/provider/kubernetes/kubernetesnamespace/v1/api.proto",
    "spec": "org/openmcf/provider/kubernetes/kubernetesnamespace/v1/spec.proto"
  },
  "spec": {
    "name": "KubernetesNamespaceSpec",
    "fields": [
      {
        "name": "TargetCluster",
        "jsonName": "targetCluster",
        "protoField": "target_cluster",
        "type": {
          "kind": "message",
          "messageType": "KubernetesClusterSelector"
        },
        "description": "The Kubernetes cluster in which the namespace should be created",
        "required": false
      },
      {
        "name": "Name",
        "jsonName": "name",
        "protoField": "name",
        "type": {
          "kind": "string"
        },
        "description": "*\n The unique name of the namespace.\n This will be used as the Kubernetes namespace metadata.name.\n Must be a valid DNS label (lowercase alphanumeric and hyphens).",
        "required": false,
        "validation": {
          "minLength": 1,
          "maxLength": 63
        }
      },
      {
        "name": "Labels",
        "jsonName": "labels",
        "protoField": "labels",
        "type": {
          "kind": "map",
          "keyType": {
            "kind": "string"
          },
          "valueType": {
            "kind": "string"
          }
        },
        "description": "*\n Additional labels to be applied to the namespace.\n These are merged with standard labels (environment, team, cost-center) for\n cost allocation, monitoring, and governance.",
        "required": false
      },
      {
        "name": "Annotations",
        "jsonName": "annotations",
        "protoField": "annotations",
        "type": {
          "kind": "map",
          "keyType": {
            "kind": "string"
          },
          "valueType": {
            "kind": "string"
          }
        },
        "description": "*\n Additional annotations to be applied to the namespace.\n Common use cases:\n - linkerd.io/inject: \"enabled\" for service mesh injection\n - janitor/ttl: \"24h\" for ephemeral namespace cleanup\n - scheduler.alpha.kubernetes.io/node-selector: for node affinity",
        "required": false
      },
      {
        "name": "ResourceProfile",
        "jsonName": "resourceProfile",
        "protoField": "resource_profile",
        "type": {
          "kind": "message",
          "messageType": "KubernetesNamespaceResourceProfile"
        },
        "description": "*\n Resource allocation profile for the namespace.\n This abstracts ResourceQuota and LimitRange configuration into T-shirt sizes\n or allows custom specifications for advanced users.",
        "required": false
      },
      {
        "name": "NetworkConfig",
        "jsonName": "networkConfig",
        "protoField": "network_config",
        "type": {
          "kind": "message",
          "messageType": "KubernetesNamespaceNetworkConfig"
        },
        "description": "*\n Network isolation and security configuration.\n Controls ingress/egress network policies to enforce zero-trust networking.",
        "required": false
      },
      {
        "name": "ServiceMeshConfig",
        "jsonName": "serviceMeshConfig",
        "protoField": "service_mesh_config",
        "type": {
          "kind": "message",
          "messageType": "KubernetesNamespaceServiceMeshConfig"
        },
        "description": "*\n Service mesh integration configuration.\n Enables automatic sidecar injection and mesh-specific features.",
        "required": false
      },
      {
        "name": "PodSecurityStandard",
        "jsonName": "podSecurityStandard",
        "protoField": "pod_security_standard",
        "type": {
          "kind": "enum",
          "enumType": "org.openmcf.provider.kubernetes.kubernetesnamespace.v1.KubernetesNamespacePodSecurityStandard",
          "enumValues": [
            "privileged",
            "baseline",
            "restricted"
          ]
        },
        "description": "*\n Pod security standards enforcement level.\n Defines the security posture for pods running in the namespace.",
        "required": false
      }
    ]
  },
