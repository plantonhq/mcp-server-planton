    {
      "name": "KubernetesSecretBasicAuthData",
      "description": "*\n **KubernetesSecretBasicAuthData** defines data for a basic authentication secret.\n Used for services that require username/password authentication.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetessecret.v1.KubernetesSecretBasicAuthData",
      "fields": [
        {
          "name": "Username",
          "jsonName": "username",
          "protoField": "username",
          "type": {
            "kind": "string"
          },
          "description": "*\n Username for basic authentication.\n Stored as the \"username\" key in the Kubernetes Secret.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        },
        {
          "name": "Password",
          "jsonName": "password",
          "protoField": "password",
          "type": {
            "kind": "string"
          },
          "description": "*\n Password for basic authentication.\n Stored as the \"password\" key in the Kubernetes Secret.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        }
      ]
    },
    {
      "name": "KubernetesSecretDockerConfigJsonData",
      "description": "*\n **KubernetesSecretDockerConfigJsonData** defines data for a Docker registry secret.\n Used for authenticating with container registries during image pulls.\n The IaC module constructs the .dockerconfigjson JSON from these fields.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetessecret.v1.KubernetesSecretDockerConfigJsonData",
      "fields": [
        {
          "name": "RegistryServer",
          "jsonName": "registryServer",
          "protoField": "registry_server",
          "type": {
            "kind": "string"
          },
          "description": "*\n Docker registry server URL (e.g., \"https://index.docker.io/v1/\", \"gcr.io\", \"ghcr.io\").",
          "required": false,
          "validation": {
            "minLength": 1
          }
        },
        {
          "name": "Username",
          "jsonName": "username",
          "protoField": "username",
          "type": {
            "kind": "string"
          },
          "description": "*\n Username for registry authentication.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        },
        {
          "name": "Password",
          "jsonName": "password",
          "protoField": "password",
          "type": {
            "kind": "string"
          },
          "description": "*\n Password or access token for registry authentication.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        },
        {
          "name": "Email",
          "jsonName": "email",
          "protoField": "email",
          "type": {
            "kind": "string"
          },
          "description": "*\n Optional email associated with the registry account.",
          "required": false
        }
      ]
    },
    {
      "name": "KubernetesSecretOpaqueData",
      "description": "*\n **KubernetesSecretOpaqueData** defines data for an Opaque secret.\n This is the most common secret type, used for arbitrary key-value pairs.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetessecret.v1.KubernetesSecretOpaqueData",
      "fields": [
        {
          "name": "Data",
          "jsonName": "data",
          "protoField": "data",
          "type": {
            "kind": "map",
            "keyType": {
              "kind": "string"
            },
            "valueType": {
              "kind": "string"
            }
          },
          "description": "*\n Key-value pairs of secret data.\n Values are plain strings (Kubernetes stringData semantics).\n At least one entry is required.",
          "required": false,
          "validation": {
            "minItems": 1
          }
        }
      ]
    },
    {
      "name": "KubernetesSecretSshAuthData",
      "description": "*\n **KubernetesSecretSshAuthData** defines data for an SSH authentication secret.\n Used for Git operations, SSH tunneling, and other SSH-based authentication.",
