        {
          "name": "SshPrivateKey",
          "jsonName": "sshPrivateKey",
          "protoField": "ssh_private_key",
          "type": {
            "kind": "string"
          },
          "description": "*\n PEM-encoded SSH private key.\n Stored as the \"ssh-privatekey\" key in the Kubernetes Secret.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        }
      ]
    },
    {
      "name": "KubernetesSecretTlsData",
      "description": "*\n **KubernetesSecretTlsData** defines data for a TLS secret.\n Used for storing TLS certificates and private keys, commonly consumed by\n Ingress controllers and other TLS-terminating services.",
      "protoType": "org.openmcf.provider.kubernetes.kubernetessecret.v1.KubernetesSecretTlsData",
      "fields": [
        {
          "name": "TlsCrt",
          "jsonName": "tlsCrt",
          "protoField": "tls_crt",
          "type": {
            "kind": "string"
          },
          "description": "*\n PEM-encoded TLS certificate (or certificate chain).\n Stored as the \"tls.crt\" key in the Kubernetes Secret.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        },
        {
          "name": "TlsKey",
          "jsonName": "tlsKey",
          "protoField": "tls_key",
          "type": {
            "kind": "string"
          },
          "description": "*\n PEM-encoded TLS private key.\n Stored as the \"tls.key\" key in the Kubernetes Secret.",
          "required": false,
          "validation": {
            "minLength": 1
          }
        }
      ]
    }
