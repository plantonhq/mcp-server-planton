  "spec": {
    "name": "KubernetesSecretSpec",
    "fields": [
      {
        "name": "TargetCluster",
        "jsonName": "targetCluster",
        "protoField": "target_cluster",
        "type": {
          "kind": "message",
          "messageType": "KubernetesClusterSelector"
        },
        "description": "The Kubernetes cluster in which the secret should be created.",
        "required": false
      },
      {
        "name": "Name",
        "jsonName": "name",
        "protoField": "name",
        "type": {
          "kind": "string"
        },
        "description": "*\n The name of the Kubernetes Secret.\n Must be a valid DNS subdomain name (lowercase alphanumeric, hyphens, and dots).",
        "required": false,
        "validation": {
          "minLength": 1,
          "maxLength": 253
        }
      },
      {
        "name": "Namespace",
        "jsonName": "namespace",
        "protoField": "namespace",
        "type": {
          "kind": "string"
        },
        "description": "*\n The namespace where the secret will be created.\n Default: default",
        "required": false,
        "validation": {
          "maxLength": 63
        },
        "default": "default",
        "oneofGroup": "_namespace"
      },
      {
        "name": "Labels",
        "jsonName": "labels",
        "protoField": "labels",
        "type": {
          "kind": "map",
          "keyType": {
            "kind": "string"
          },
          "valueType": {
            "kind": "string"
          }
        },
        "description": "*\n Additional labels to apply to the secret.\n These are merged with standard OpenMCF labels for resource tracking and governance.",
        "required": false
      },
      {
        "name": "Annotations",
        "jsonName": "annotations",
        "protoField": "annotations",
        "type": {
          "kind": "map",
          "keyType": {
            "kind": "string"
          },
          "valueType": {
            "kind": "string"
          }
        },
        "description": "*\n Additional annotations to apply to the secret.",
        "required": false
      },
      {
        "name": "Immutable",
        "jsonName": "immutable",
        "protoField": "immutable",
        "type": {
          "kind": "bool"
        },
        "description": "*\n When true, the secret data cannot be updated after creation.\n Immutable secrets provide protection against accidental updates and\n improve cluster performance by reducing watch load on the API server.",
        "required": false
      },
      {
        "name": "Opaque",
        "jsonName": "opaque",
        "protoField": "opaque",
        "type": {
          "kind": "message",
          "messageType": "KubernetesSecretOpaqueData"
        },
        "description": "Opaque secret with arbitrary key-value string data.\n Maps to Kubernetes secret type \"Opaque\".",
        "required": false,
        "oneofGroup": "secret_data"
      },
      {
        "name": "Tls",
        "jsonName": "tls",
        "protoField": "tls",
        "type": {
          "kind": "message",
          "messageType": "KubernetesSecretTlsData"
        },
        "description": "TLS secret containing a certificate and private key.\n Maps to Kubernetes secret type \"kubernetes.io/tls\".",
        "required": false,
        "oneofGroup": "secret_data"
      },
      {
        "name": "DockerConfigJson",
        "jsonName": "dockerConfigJson",
        "protoField": "docker_config_json",
        "type": {
          "kind": "message",
          "messageType": "KubernetesSecretDockerConfigJsonData"
        },
        "description": "Docker registry credentials for image pulls.\n Maps to Kubernetes secret type \"kubernetes.io/dockerconfigjson\".",
        "required": false,
        "oneofGroup": "secret_data"
      },
      {
        "name": "BasicAuth",
        "jsonName": "basicAuth",
        "protoField": "basic_auth",
        "type": {
          "kind": "message",
          "messageType": "KubernetesSecretBasicAuthData"
        },
        "description": "Basic authentication credentials (username/password).\n Maps to Kubernetes secret type \"kubernetes.io/basic-auth\".",
        "required": false,
        "oneofGroup": "secret_data"
      },
      {
        "name": "SshAuth",
        "jsonName": "sshAuth",
        "protoField": "ssh_auth",
        "type": {
          "kind": "message",
          "messageType": "KubernetesSecretSshAuthData"
        },
        "description": "SSH authentication credentials (private key).\n Maps to Kubernetes secret type \"kubernetes.io/ssh-auth\".",
        "required": false,
        "oneofGroup": "secret_data"
      }
    ]
  },
