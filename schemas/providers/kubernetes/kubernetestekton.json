    {
      "name": "KubernetesTektonCloudEvents",
      "description": "KubernetesTektonCloudEvents configures CloudEvents integration for Tekton.\n CloudEvents are sent for TaskRun and PipelineRun state changes, enabling\n external systems to react to pipeline events.\n\n This configuration is applied to the 'config-defaults' ConfigMap in the tekton-pipelines namespace.\n See: https://tekton.dev/docs/pipelines/additional-configs/#configuring-cloudevents-notifications",
      "protoType": "org.openmcf.provider.kubernetes.kubernetestekton.v1.KubernetesTektonCloudEvents",
      "fields": [
        {
          "name": "SinkUrl",
          "jsonName": "sinkUrl",
          "protoField": "sink_url",
          "type": {
            "kind": "string"
          },
          "description": "The URL where CloudEvents will be sent.\n Must be a valid HTTP/HTTPS URL that can receive CloudEvents.\n Example: \"http://my-service.my-namespace.svc.cluster.local/tekton/events\"",
          "required": false
        }
      ]
    },
