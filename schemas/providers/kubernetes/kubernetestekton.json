{
  "name": "KubernetesTekton",
  "kind": "KubernetesTekton",
  "cloudProvider": "kubernetes",
  "apiVersion": "kubernetes.openmcf.org/v1",
  "description": "KubernetesTekton deploys Tekton Pipelines and Dashboard on Kubernetes using official release manifests.\n\n This is the manifest-based deployment approach (as opposed to the operator-based KubernetesTektonOperator).\n It's simpler to understand and debug, and gives direct control over configuration.",
  "protoPackage": "org.openmcf.provider.kubernetes.kubernetestekton.v1",
  "protoFiles": {
    "api": "org/openmcf/provider/kubernetes/kubernetestekton/v1/api.proto",
    "spec": "org/openmcf/provider/kubernetes/kubernetestekton/v1/spec.proto"
  },
  "spec": {
    "name": "KubernetesTektonSpec",
    "fields": [
      {
        "name": "TargetCluster",
        "jsonName": "targetCluster",
        "protoField": "target_cluster",
        "type": {
          "kind": "message",
          "messageType": "KubernetesClusterSelector"
        },
        "description": "Target Kubernetes Cluster where Tekton will be deployed.",
        "required": false
      },
      {
        "name": "PipelineVersion",
        "jsonName": "pipelineVersion",
        "protoField": "pipeline_version",
        "type": {
          "kind": "string"
        },
        "description": "The version of Tekton Pipelines to deploy.\n This maps to release versions from https://github.com/tektoncd/pipeline/releases\n Examples: \"latest\", \"v0.65.2\", \"v0.64.0\"",
        "required": false,
        "default": "latest"
      },
      {
        "name": "Dashboard",
        "jsonName": "dashboard",
        "protoField": "dashboard",
        "type": {
          "kind": "message",
          "messageType": "KubernetesTektonDashboard"
        },
        "description": "Dashboard configuration for Tekton.\n The dashboard provides a web UI for viewing and managing pipelines.",
        "required": false
      },
      {
        "name": "CloudEvents",
        "jsonName": "cloudEvents",
        "protoField": "cloud_events",
        "type": {
          "kind": "message",
          "messageType": "KubernetesTektonCloudEvents"
        },
        "description": "Cloud Events configuration for pipeline notifications.\n When configured, Tekton will send CloudEvents to the specified sink URL\n for TaskRun and PipelineRun lifecycle events.",
        "required": false
      }
    ]
  },
